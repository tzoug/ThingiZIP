{
  "name": "ThingiZIP",
  "description": "A browser extension to easily download files from Thingiverseâ„¢",
  "version": "3.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "permissions": [
    "storage"
  ],
  "optional_permissions": [
    "activeTab"
  ],
  "icons": {
    "48": "ThingiZIP.png"
  },  
  "content_scripts":[
    {
      "matches": ["https://www.thingiverse.com/thing:*"],
      "css": ["./src/dummy.css"]
    }
  ]
}